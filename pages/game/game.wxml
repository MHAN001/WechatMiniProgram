<view class="bg-blue">
  <view class="">You have {{foods}} foods available</view>
  <view>
    <button class="btn-primary" bindtap="feed">feed</button>
    <button class="btn-primary" bindtap="getSteps">Foods</button>
    <view> <!--page-section page-section-gap -->
      <view wx:for="{{imgUrls}}"  wx:if="{{index == imgId}}" ><!--  -->
        <image class="cat"  src = "{{item}}"/>
      </view>
    </view>
  </view>
  <movable-area style="height: 215px; width: 100%; background: green; opacity: 0.5;">
    <canvas class="ball" show="{{true}}" canvas-id="big-ball"></canvas>
    <movable-view direction="all" style="height: 30; width: 30px; background: blue; margin-top: 100%; margin-left: 50%;" x="{{a}}" y="{{b}}">
      <canvas class="ball" show="{{true}}" canvas-id="small-ball"></canvas>
    </movable-view>
  </movable-area>
</view>
